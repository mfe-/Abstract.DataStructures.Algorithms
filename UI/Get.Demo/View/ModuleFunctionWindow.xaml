<Window x:Class="DataStructures.Demo.View.ModuleFunctionWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DataStructures.Demo.View"
        mc:Ignorable="d"
        Title="ModuleFunctionWindow" Height="450" Width="800">
    <DockPanel LastChildFill="False" >
        <Button Content="Select assembly" Command="{Binding PickAssemblyCommand}"></Button>
        <ListBox></ListBox>
        <TextBox Text="{Binding Path=ModuleFunction.Description,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}" />
        <Button Content="Save" />
    </DockPanel>
</Window>
